  <template id="statement_report_pdf">
    <t t-call="web.html_container">
      <t t-foreach="docs" t-as="doc">
        <t t-call="web.external_layout">
          <div class="page">
            <h2 t-field="doc.date_start"/>
            <h2 t-field="doc.date_end"/>
          </div>
        </t>
      </t>
    </t>
  </template>

</odoo>